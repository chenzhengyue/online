alias l="ls -l"
alias ll="ls -lrt"
alias la="ls -la"
alias cdo="cd $HOME/online"
alias cdb="cd $HOME/online/bin"
alias cdlib="cd $HOME/online/lib"
alias cdsb="cd $HOME/online/sbin"
alias cde="cd $HOME/online/etc"
alias cdl="cd $HOME/online/log"
alias cdi="cd $HOME/online/include"
alias cds="cd $HOME/online/src"
alias cdm="cd $HOME/online/mak"
alias findall="find . -name \"*\" | xargs grep"
alias findc="find . -name \"*.c\" | xargs grep"
alias findh="find . -name \"*.h\" | xargs grep"
